{
    "name": "digitickets/digitickets-php-api-client",
    "description": "Provides access to the DigiTickets API.",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "DigiTickets",
            "email": "developers@digitickets.co.uk"
        }
    ],
    "autoload": {
        "psr-4": {
            "DigiTicketsApiClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DigiTicketsApiClientTests\\": "tests/"
        }
    },
    "require": {
        "digitickets/generic-api-client": "^1.0.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^6",
        "vlucas/phpdotenv": "^4.2"
    },
    "scripts": {
        "test": "vendor/bin/phpunit tests --colors"
    }
}
